{
  "expo": {
    "name": "Green Up",
    "description": "The Official App for Vermont's Green Up Day",
    "slug": "Green-Up-Vermont",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "owner": "c4btv",
    "githubUrl": "https://github.com/codeforbtv/green-up-app",
